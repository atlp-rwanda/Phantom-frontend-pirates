{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "func-names": "off",
      "no-void": "warn",
      "linebreak-style": "warn",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "warn"
    }
  }
  